<header class="header">
  <div class="blog-title">
    <% if (theme.custom_logo['enabled'] == true) { %>
      <img src="<%- url_for(theme.custom_logo['image']) %>" alt="logo">
    <% } %>
    <a href="<%- url_for() %>" class="logo"><%= config.title %></a>
  </div>
  <nav class="navbar">
    <ul class="menu">
      <% for (name in theme.menu) { %>
        <li class="menu-item">
          <i class="fa fa-camera-retro"></i>
          <a href="<%- url_for(theme.menu[name]) %>" class="menu-item-link"><%= __(name) %></a>
        </li>
      <% } %>
    </ul>
  </nav>
</header>
